                    "value": "={{ $node[\"Loop por Aba\"].json.sheetName }}"
                },
                "options": {}
            },
            "id": "node-read-source",
            "name": "[GSheets] Ler Aba Atual",
            "type": "n8n-nodes-base.googleSheets",
            "typeVersion": 4,
            "position": [
                800,
                200
            ],
            "alwaysOutputData": true
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $node[\"[GSheets] Ler Aba Atual\"].json.length > 0 }}",
                            "value2": true
                        }
                    ]
                }
            },
            "id": "node-check-empty",
            "name": "Tem dados?",
